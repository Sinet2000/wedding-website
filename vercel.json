{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/svadba-daria-nikita.ics",
      "headers": [
        { "key": "Content-Type", "value": "text/calendar" },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=svadba-daria-nikita.ics"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/svadba-daria-nikita.ics",
      "dest": "/svadba-daria-nikita.ics"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "github": {
    "silent": true,
    "autoAlias": true
  }
}
